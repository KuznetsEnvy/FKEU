{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}
<div class="container">
  <p class="cart-thk"><ly-as-2473735>thank you for shopping at</ly-as-2473735> <span><ly-as-2473736>floatie</ly-as-2473736></span> <ly-as-2473737>kings!</ly-as-2473737> </p>
  {% if cart.item_count == 0 %}
  
  <h2 class="page-title">{% comment %}ly_i18n_replace_for_[ 'cart.general.empty_title' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.empty_title' | t }}{% endcapture %}{% include 'ly-static-string' with '2473740' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h2>
  <div class="rte">
      
      {% comment %}ly_i18n_replace_for_[ 'cart.general.empty_subtext_html' | t ]_begin{% endcomment %}
      {% capture ly_template %}{{ 'cart.general.empty_subtext_html' | t }}{% endcapture %}
      {% include 'ly-static-string' with '2473741' %}
      {% if ly_translation %}
          {% assign escape_content = true %}
          {% assign ly_template = ly_translation %}
      {% else %}
          {% assign escape_content = false %}
      {% endif %}
      {% if escape_content %}
          {{ ly_template}}
      {% else %}
          {{ ly_template }}
      {% endif %}
      {% comment %}
          ly_i18n_replace_end
      {% endcomment %}
      
    {% comment %}
      <a href="/">Click here</a> to continue shopping.
	{% endcomment %}
	</div>

  {% else %}
  
  <div class="reading-column">
    
    <form action="/cart{% comment %}ly_checkout_begin{% endcomment %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% assign checkout_code = shop.metafields.language_codes[language] %}{% if checkout_code %}{% assign checkout_code = '?locale=' | append: checkout_code %}{{ checkout_code }}{% endif %}{% comment %}ly_checkout_end{% endcomment %}" method="post" id="cartform">

      <ul class="cart-items plain">
        <li class="heading border-bottom cf">
          <span class="description">{% comment %}ly_i18n_replace_for_[ 'cart.label.description' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.label.description' | t }}{% endcapture %}{% include 'ly-static-string' with '2473742' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
          <span class="quantity">{% comment %}ly_i18n_replace_for_[ 'cart.label.quantity' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.label.quantity' | t }}{% endcapture %}{% include 'ly-static-string' with '2473743' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
          <span class="price"><ly-as-2473738>total</ly-as-2473738>{% comment %}{{ 'cart.label.total' | t }}{% endcomment %}</span>
        </li>
        {% for item in cart.items %}
          <li class="item border-bottom cf">
            <div class="description cf">
              <a class="image" href="{{ item.url }}">
                <img src="{{ item | img_url: 'small' }}"  alt="{% comment %}ly_code_replace_for_[ item.title | escape ]_begin{% endcomment %}{% include 'ly-title' with item %}{{ ly_translation | escape }}{% comment %}ly_code_replace_end{% endcomment %}" />
              </a>
            <div class="left">
              <div>{% comment %}ly_code_replace_for_[ item.product.title | link_to: item.product.url ]_begin{% endcomment %}{% include 'ly-title' with item.product %}{{ ly_translation | link_to: item.product.url }}{% comment %}ly_code_replace_end{% endcomment %}</div>
              {% if item.product.variants.size > 1 %}
              <span class="variant">{% comment %}ly_code_replace_for_[ item.variant.title ]_begin{% endcomment %}{% include 'ly-title' with item.variant %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}</span>
              {% endif %}
              {% comment %}
              Line Item Properties
              {% endcomment %}
              {% for p in item.properties %}
              <div class="property">
                {% unless p.last == blank %}
                <span class="label">{{ p.first }}:</span>
                {% if p.last contains '/uploads/' %}
                <a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                {% else %}
                {{ p.last }}
                {% endif %}
                <br />
                {% endunless %}
              </div>
              {% endfor %}
              <p><a href="/cart/change?line={{ forloop.index }}&quantity=0" class="remove feature-col">{% comment %}ly_i18n_replace_for_[ 'cart.general.remove' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.remove' | t }}{% endcapture %}{% include 'ly-static-string' with '2473744' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a></p>
             </div> 
            </div>
            <div class="quantity">
              <input class="select-on-focus" type="text" size="2" name="updates[]" id="updates_{{ item.id }}" value="{{ item.quantity }}" />
            </div>
            <div class="price"><h5>{% assign geolizr_price =  item.line_price  %}{% include 'geolizr-currency' with geolizr_price %}{{ item.line_price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</h5></div>
          </li>
        {% endfor %}
      </ul>

      <div class="border-bottom">
        <div class="row">
          
          <div class="half column align-left" style="width:50%;float:left;">
          	<input type="submit" class="alt" id="update-cart" name="update" value="{% comment %}ly_i18n_replace_for_[ 'cart.general.update' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.update' | t }}{% endcapture %}{% include 'ly-static-string' with '2473745' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}" />
          </div>
          
          <div class="half column align-right" style="width:50%;float:right;">
            <label style="float: left;padding-top: 14px;margin-bottom: 25px;" class="desktop" for="checkout">{% comment %}ly_i18n_replace_for_[ 'cart.label.subtotal' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.label.subtotal' | t }}{% endcapture %}{% include 'ly-static-string' with '2473746' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
            <h2 style="font-size: 20px;margin-bottom: 0;padding-top: 10px;">{% assign geolizr_price =  cart.total_price  %}{% include 'geolizr-currency' with geolizr_price %}{{ cart.total_price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</h2>
          </div>
          
        </div>
      </div>
      
      
      <div class="border-bottom">
        <div class="row">
          <div class="column align-center" style="width:100%;margin-bottom: 1rem;">
      		<input style="width:100%" type="submit" id="checkout" name="checkout" value="{% comment %}ly_i18n_replace_for_[ 'cart.general.checkout' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.checkout' | t }}{% endcapture %}{% include 'ly-static-string' with '2473747' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}" />
          </div>
          {% comment %}<div class="column align-center" style="width:100%">
            <p style="font-size:12px">You will be able to use your discount code in the next step</p>
          </div>{% endcomment %}
          <div class="column align-center cont" style="width:100%; margin-top: 15px;">
            <a class="left continue" href="/" style="color: #000; border: 2px solid black; padding: 15px; margin-top: 15px;"><ly-as-2473739>keep shopping</ly-as-2473739>{% comment %}{% comment %}ly_i18n_replace_for_[ 'cart.general.continue' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.continue' | t }}{% endcapture %}{% include 'ly-static-string' with '2473748' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}{% endcomment %}</a>
          </div>
        </div>
      </div>
      
      {% comment %}
      <div class="spaced align-right">
      </div>

      <div class="border-bottom">
        <div class="row">
          <div class="half column align-right desktop">
            <label for="checkout">{% comment %}ly_i18n_replace_for_[ 'cart.label.subtotal' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.label.subtotal' | t }}{% endcapture %}{% include 'ly-static-string' with '2473746' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
          </div>
        </div>
      </div>
      <div class="row section">
        <div class="half column align-right">
          <label class="mobile" for="checkout">{% comment %}ly_i18n_replace_for_[ 'cart.label.subtotal' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.label.subtotal' | t }}{% endcapture %}{% include 'ly-static-string' with '2473746' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
          
          <h2>{% assign geolizr_price =  cart.total_price  %}{% include 'geolizr-currency' with geolizr_price %}{{ cart.total_price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</h2>
          
          <input type="submit" id="checkout" name="checkout" value="{% comment %}ly_i18n_replace_for_[ 'cart.general.checkout' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.general.checkout' | t }}{% endcapture %}{% include 'ly-static-string' with '2473747' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}" />
        </div>
      </div>
      {% endcomment %}

    </form>

  </div>
  
  {% endif %}
  
</div>