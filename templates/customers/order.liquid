{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<div id="customer-order" class="container account-area">
  <h1 class="page-title">
    {% comment %}ly_i18n_replace_for_[ 'customer.order.title' | t: name: order.name ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.title' | t: name: order.name }}{% endcapture %}{% include 'ly-static-string' with '2473798' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = order.name  %}{% assign ly_template = ly_template | replace: '[[name]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
    <span class="note order_date">&mdash; {{ order.created_at | date: format: 'month_date_year' }}</span>
  </h1>

  <a class="back-link" href="/account"><i class="icon-arrow3-left slim"></i> <span>{% comment %}ly_i18n_replace_for_[ 'customer.general.back_to_account' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.general.back_to_account' | t }}{% endcapture %}{% include 'ly-static-string' with '2473799' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span></a>
  
  {% if order.cancelled %}
  <div id="order_cancelled" class="flash notice">
    <h5 id="order_cancelled_title">{% comment %}ly_i18n_replace_for_[ 'customer.order.cancelled' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.cancelled' | t }}{% endcapture %}{% include 'ly-static-string' with '2473800' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} <span class="note">{{ order.cancelled_at | date: format: 'month_date_year' }}</span></h5>
    <span class="note">{% comment %}ly_i18n_replace_for_[ 'customer.order.cancelled_reason' | t: reason: order.cancel_reason ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.cancelled_reason' | t: reason: order.cancel_reason }}{% endcapture %}{% include 'ly-static-string' with '2473802' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = order.cancel_reason  %}{% assign ly_template = ly_template | replace: '[[reason]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
  </div>
  {% endif %}

  <div class="reading-column">

    <div class="row">
      <div id="order_payment" class="half column">
        <h2 class="order_section_title">{% comment %}ly_i18n_replace_for_[ 'customer.order.billing_address' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.billing_address' | t }}{% endcapture %}{% include 'ly-static-string' with '2473803' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h2>
        <p><span class="note">{% comment %}ly_i18n_replace_for_[ 'customer.order.payment_status' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.payment_status' | t }}{% endcapture %}{% include 'ly-static-string' with '2473804' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}:</span> <span class="status_{{ order.financial_status }}">{{ order.financial_status_label }}</span></p>
        <div class="address note">
          <div>{{ order.billing_address.name }}</div>
          <div>{{ order.billing_address.company }}</div>
          <div>{{ order.billing_address.street }}</div>
          <div>{{ order.billing_address.city }}</div>
          <div>{{ order.billing_address.province }}</div>
          <div>{{ order.billing_address.country }} {{ order.billing_address.zip }}</div>
		  <div>{{ order.billing_address.phone }}</div>
        </div>
      </div>
      {% if order.shipping_address %}
      <div id="order_shipping" class="half column">
        <h2 class="order_section_title">{% comment %}ly_i18n_replace_for_[ 'customer.order.shipping_address' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.shipping_address' | t }}{% endcapture %}{% include 'ly-static-string' with '2473805' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h2>
        <p><span class="note">{% comment %}ly_i18n_replace_for_[ 'customer.order.fulfillment_status' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.fulfillment_status' | t }}{% endcapture %}{% include 'ly-static-string' with '2473806' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}:</span> <span class="status_{{ order.fulfillment_status }}">{{ order.fulfillment_status_label }}</span></p>
        <div class="address note">
          <div>{{ order.shipping_address.name }}</div>
          <div>{{ order.shipping_address.company }}</div>
          <div>{{ order.shipping_address.street }}</div>
          <div>{{ order.shipping_address.city }}</div>
          <div>{{ order.shipping_address.province }}</div>
          <div>{{ order.shipping_address.country }} {{ order.shipping_address.zip }}</div>
          <div>{{ order.shipping_address.phone }}</div>
        </div>
      </div>
      {% endif %}

    </div>

    <div class="section rte">
      <table id="order_details" class="fullwidth">
        <thead>
          <tr>
            <th>{% comment %}ly_i18n_replace_for_[ 'customer.order.product' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.product' | t }}{% endcapture %}{% include 'ly-static-string' with '2473807' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</th>
            <th>{% comment %}ly_i18n_replace_for_[ 'customer.order.sku' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.sku' | t }}{% endcapture %}{% include 'ly-static-string' with '2473808' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</th>
            <th>{% comment %}ly_i18n_replace_for_[ 'customer.order.price' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.price' | t }}{% endcapture %}{% include 'ly-static-string' with '2473809' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</th>
            <th class="center">{% comment %}ly_i18n_replace_for_[ 'customer.order.quantity' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.quantity' | t }}{% endcapture %}{% include 'ly-static-string' with '2473810' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</th>
            <th class="total">{% comment %}ly_i18n_replace_for_[ 'customer.order.total' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.total' | t }}{% endcapture %}{% include 'ly-static-string' with '2473811' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</th>
          </tr>
        </thead>
        <tbody>
          {% for line_item in order.line_items %}
          <tr id="{{ line_item.id }}" class="{% cycle 'odd', 'even' %}">
            <td class="">
              {% comment %}ly_code_replace_for_[ line_item.title | link_to: line_item.product.url ]_begin{% endcomment %}{% include 'ly-title' with line_item %}{{ ly_translation | link_to: line_item.product.url }}{% comment %}ly_code_replace_end{% endcomment %}
              {% if line_item.fulfillment %}
              <div class="note">
                {% comment %}ly_i18n_replace_for_[ 'customer.order.fulfilled' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.fulfilled' | t }}{% endcapture %}{% include 'ly-static-string' with '2473812' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}  {{ line_item.fulfillment.created_at | date: format: 'month_date_year' }}
                {% if line_item.fulfillment.tracking_number %}
                <a href="{{ line_item.fulfillment.tracking_url }}">{{ line_item.fulfillment.tracking_company }} #{{ line_item.fulfillment.tracking_number}}</a>
                {% endif %}
              </div>
              {% endif %}
            </td>
            <td class="sku note">{{ line_item.sku }}</td>
            <td class="money">{% assign geolizr_price =  line_item.price  %}{% include 'geolizr-currency' with geolizr_price %}{{ line_item.price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</td>
            <td class="quantity cente">{{ line_item.quantity }}</td>
            <td class="total money">{% assign geolizr_price =  line_item.quantity | times: line_item.price  %}{% include 'geolizr-currency' with geolizr_price %}{{ line_item.quantity | times: line_item.price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</td>
          </tr>
          {% endfor %}
        </tbody>
        <tfoot>
          <tr class="order_summary note">
            <td class="label" colspan="4">{% comment %}ly_i18n_replace_for_[ 'customer.order.subtotal' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.subtotal' | t }}{% endcapture %}{% include 'ly-static-string' with '2473813' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</td>
            <td class="total money">{% assign geolizr_price =  order.subtotal_price  %}{% include 'geolizr-currency' with geolizr_price %}{{ order.subtotal_price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</td>
          </tr>
          
          {% for discount in order.discounts %}
          <tr class="order_summary discount">
            <td colspan="4">{{ discount.code }} {% comment %}ly_i18n_replace_for_[ 'customer.order.discount' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.discount' | t }}{% endcapture %}{% include 'ly-static-string' with '2473814' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</td>
            <td class="text-right">{% assign geolizr_price =  discount.savings  %}{% include 'geolizr-currency' with geolizr_price %}{{ discount.savings | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</td>
          </tr>
          {% endfor %}
          
          {% for shipping_method in order.shipping_methods %}
          <tr class="order_summary note">
            <td class="label" colspan="4">{% comment %}ly_code_replace_for_[ shipping_method.title ]_begin{% endcomment %}{% include 'ly-title' with shipping_method %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}:</td>
            <td class="total money">{% assign geolizr_price =  shipping_method.price  %}{% include 'geolizr-currency' with geolizr_price %}{{ shipping_method.price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</td>
          </tr>
          {% endfor %}

          {% for tax_line in order.tax_lines %}
          <tr class="order_summary note">
            <td class="label" colspan="4">{% comment %}ly_code_replace_for_[ tax_line.title ]_begin{% endcomment %}{% include 'ly-title' with tax_line %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %} {{ tax_line.rate | times: 100 }}%:</td>
            <td class="total money">{% assign geolizr_price =  tax_line.price  %}{% include 'geolizr-currency' with geolizr_price %}{{ tax_line.price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }}</td>
          </tr>
          {% endfor %}

          <tr class="order_summary order_total">
            <td class="label" colspan="4">{% comment %}ly_i18n_replace_for_[ 'customer.order.total' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.total' | t }}{% endcapture %}{% include 'ly-static-string' with '2473811' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</td>
            <td class="total money">{% assign geolizr_price =  order.total_price  %}{% include 'geolizr-currency' with geolizr_price %}{{ order.total_price | money | prepend: geolizr_prepend_code | append: geolizr_append_code }} {{ order.currency }}</td>
          </tr>
        </tfoot>
      </table>
    </div>
  </div>
</div>