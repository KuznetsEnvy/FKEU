{% if paginate.pages > 1 %}
<div class="border-top">
  <div class="container">
    <div class="inline-row cf">
      <p class="inline-item left"><span class="results-count">{% comment %}ly_i18n_replace_for_[ 'collections.general.results' | t: count: collection.products_count ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.general.results' | t: count: collection.products_count }}{% endcapture %}{% include 'ly-static-string' with '2473584' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = collection.products_count  %}{% assign ly_template = ly_template | replace: '[[count]]', replacement %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span></p>
      <div class="inline-item right last">{% include 'pagination' %}</div>
    </div>
  </div>
</div>
{% endif %}

{% endpaginate %}

<script>
  Shopify.queryParams = {};
  if (location.search.length) {
    for (var aKeyValue, i = 0, aCouples = location.search.substr(1).split('&'); i < aCouples.length; i++) {
      aKeyValue = aCouples[i].split('=');
      if (aKeyValue.length > 1) {
        Shopify.queryParams[decodeURIComponent(aKeyValue[0])] = decodeURIComponent(aKeyValue[1]);
      }
    }
  }
  jQuery('.sort-by')
    .val('{{ collection.sort_by | default: collection.default_sort_by  }}')
    .bind('change', function() {
      Shopify.queryParams.sort_by = jQuery(this).val();
      location.search = jQuery.param(Shopify.queryParams).replace(/\+/g, '%20');
    });
</script>

{% if settings.coll_infiniscroll %}
{% comment %}ly_asset_replace_for_[ 'jquery.infinitescroll.min.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'jquery.infinitescroll.min.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
<script>
  $(window).on('load', function(){
    $('.product-list').infinitescroll({
      navSelector  : ".pagination",
      nextSelector : ".pagination .next",
      itemSelector : ".product-list .product-block",
      loading: {
        img         : {% comment %}ly_asset_replace_for_[ 'loading.gif' | asset_url | json ]_begin{% endcomment %}{% assign ly_asset = 'loading.gif' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | json }}{% comment %}ly_asset_replace_end{% endcomment %},
        msgText     : {% comment %}ly_i18n_replace_for_[ 'collections.infinite_scroll.loading' | t | json ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.infinite_scroll.loading' | t | json }}{% endcapture %}{% include 'ly-static-string' with '2473598' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | json }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %},
        finishedMsg : {% comment %}ly_i18n_replace_for_[ 'collections.infinite_scroll.no_more' | t | json ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.infinite_scroll.no_more' | t | json }}{% endcapture %}{% include 'ly-static-string' with '2473599' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | json }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
      },
      pathParse:function(path,nextPage){
        return path.match(/^(.*page=)[0-9]*(&.*)?$/).splice(1);
      }
    }, function(newElements){
      $(newElements).imagesLoaded(function(){
        $(window).trigger('resize');
      });
    });
  });
</script>
{% endif %}