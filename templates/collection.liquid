            <option value="{{ collection_url }}">{% comment %}ly_i18n_replace_for_[ 'collections.general.all_items' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.general.all_items' | t }}{% endcapture %}{% include 'ly-static-string' with '2473586' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
