{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<div class="onboarding">
  <div class="onboarding-behind">
    <div class="opposing-items">
      <h1 class="section-heading left">{% comment %}ly_i18n_replace_for_[ "home_page.onboarding.blog_title" | t ]_begin{% endcomment %}{% capture ly_template %}{{ "home_page.onboarding.blog_title" | t }}{% endcapture %}{% include 'ly-static-string' with '4456820' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h1>
      
      <span class="btn">{% comment %}ly_i18n_replace_for_[ 'collections.general.view_all' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.general.view_all' | t }}{% endcapture %}{% include 'ly-static-string' with '4456818' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
    </div>

    <div class="row">

      {% for i in (1..3) %}

      {% assign mod4 = forloop.index | modulo: 4 %}
      {% assign mod3 = forloop.index | modulo: 3 %}
      {% assign mod2 = forloop.index | modulo: 2 %}
      
      <div class="article third column {% if mod2 == 1 %} nth-2{% endif %}{% if mod3 == 1 %} nth-3{% endif %}{% if mod4 == 1 %} nth-4{% endif %}">
        
        <div class="header-image">{% comment %}ly_asset_replace_for_[ 'plain_square.gif' | asset_url | img_tag ]_begin{% endcomment %}{% assign ly_asset = 'plain_square.gif' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | img_tag }}{% comment %}ly_asset_replace_end{% endcomment %}</div>
        
        <div class="reading-column">
          <h2>{% comment %}ly_i18n_replace_for_[ "home_page.onboarding.article_title" | t ]_begin{% endcomment %}{% capture ly_template %}{{ "home_page.onboarding.article_title" | t }}{% endcapture %}{% include 'ly-static-string' with '4456821' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h2>

          <div class="excerpt rte">
            {% comment %}ly_i18n_replace_for_[ "home_page.onboarding.article_excerpt" | t ]_begin{% endcomment %}{% capture ly_template %}{{ "home_page.onboarding.article_excerpt" | t }}{% endcapture %}{% include 'ly-static-string' with '4456822' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
          </div>

          <a class="more-link">{% comment %}ly_i18n_replace_for_[ 'blogs.general.read_more' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'blogs.general.read_more' | t }}{% endcapture %}{% include 'ly-static-string' with '4456819' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a>
        </div>
        
      </div>

      {% endfor %}

    </div>
  </div>
  <div class="onboarding-overlay">{% comment %}ly_i18n_replace_for_[ "home_page.onboarding.blog_instruction_html" | t ]_begin{% endcomment %}{% capture ly_template %}{{ "home_page.onboarding.blog_instruction_html" | t }}{% endcapture %}{% include 'ly-static-string' with '4456823' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</div>
</div>
