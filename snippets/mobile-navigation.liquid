{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<nav id="mobile-nav">
  <div class="inner">
	
    {% if settings.show_header_search %}
    <form class="search-form" action="/search" method="get">
      {% if settings.search_prods_only %}<input type="hidden" name="type" value="product">{% endif %}
      <button class="icon-mag"></button>
      <input type="text" name="q" placeholder="{% comment %}ly_i18n_replace_for_[ 'layout.header.search' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.header.search' | t }}{% endcapture %}{% include 'ly-static-string' with '2473646' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}"/>
    </form>
    {% endif %}
    
    <div class="header">
      {% include 'social-links' %}
    </div>

    {% comment %} Start of main menu {% endcomment %}
    <ul class="plain">
      {% for link in linklists.main-menu.links %}
      
        {% assign has_drop_down = false %}
        {% capture link_title %}{% comment %}ly_code_replace_for_[ link.title | strip_html ]_begin{% endcomment %}{% include 'ly-title' with link %}{{ ly_translation | strip_html }}{% comment %}ly_code_replace_end{% endcomment %}{% endcapture %}
        {% if link_title == settings.nav_uber_name %}
          {% assign dropdown_style = 'drop-uber' %}
          {% assign has_drop_down = true %}
        {% else %}
          {% include 'has-drop-down' with link_title %}
          {% if has_drop_down %}{% assign dropdown_style = 'drop-norm' %}{% endif %}
        {% endif %}
      
        <li class="nav-item{% if link.active %} active{% endif %}">
          <a class="nav-item-link" href="{{ link.url }}">
            {% comment %}ly_code_replace_for_[ link.title ]_begin{% endcomment %}{% include 'ly-title' with link %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}
          </a>

          {% if has_drop_down and dropdown_style == 'drop-norm' %}
          <button class="open-sub-nav alt" data-sub-nav="{{ child_list_handle }}"><i class="icon-arrow3-right"></i></button>
          
          {% elsif has_drop_down and dropdown_style == 'drop-uber' %}
          <button class="open-sub-nav alt" data-sub-nav="mega-nav"><i class="icon-arrow3-right"></i></button>
            {% if dropdown_style == 'drop-uber' %}
            {% capture uber_subnav %}
            <div class="sub-nav" data-is-subnav-for="mega-nav">
              <div class="header">
                <a class="close-sub-nav" href="#"><span class="btn alt"><i class="icon-arrow3-left slim"></i></span> {% comment %}ly_i18n_replace_for_[ 'layout.mobile_navigation.back' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.mobile_navigation.back' | t }}{% endcapture %}{% include 'ly-static-string' with '2473647' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a>
              </div>

              <ul class="sub-nav-list plain">
                {% for i in (1..4) %}
                {% capture uber_list_title_key %}nav_uber_list_{{i}}_title{% endcapture %}
                {% capture uber_list_list_key %}nav_uber_list_{{i}}{% endcapture %}
                {% if settings[uber_list_list_key] != blank %}
                <li class="sub-nav-item">
                  <span class="sub-nav-item-link">{{ settings[uber_list_title_key] }}</span>
                  <button class="open-sub-nav alt" data-sub-nav="{{ settings[uber_list_list_key] }}"><i class="icon-arrow3-right"></i></button>
                </li>
                {% endif %}
                {% endfor %}
              </ul>

            </div>
            {% endcapture %}
            {% endif %}
          {% endif %}
        </li>
      {% endfor %}

      {% if shop.customer_accounts_enabled %}
        {% if customer  %}
          <li class="account-nav-item border-top"><a href="/account">{% comment %}ly_i18n_replace_for_[ 'layout.customer.your_account' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.customer.your_account' | t }}{% endcapture %}{% include 'ly-static-string' with '2473648' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a></li>
          <li class="account-nav-item">{% comment %}ly_i18n_replace_for_[ 'layout.customer.logout' | t | customer_logout_link ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.customer.logout' | t | customer_logout_link }}{% endcapture %}{% include 'ly-static-string' with '2473649' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | customer_logout_link }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</li>
        {% else %}
            <li class="account-nav-item border-top">{% comment %}ly_i18n_replace_for_[ 'layout.customer.login' | t | customer_login_link ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.customer.login' | t | customer_login_link }}{% endcapture %}{% include 'ly-static-string' with '2473650' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | customer_login_link }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</li>
          {% if shop.customer_accounts_optional %}
            <li class="account-nav-item">{% comment %}ly_i18n_replace_for_[ 'layout.customer.create_account' | t | customer_register_link ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.customer.create_account' | t | customer_register_link }}{% endcapture %}{% include 'ly-static-string' with '2473651' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | customer_register_link }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</li>
          {% endif %}
        {% endif %}
      {% endif %}
    </ul>
  </div>
  
  {% comment %} Output sub-nav {% endcomment %}
  {% for link in linklists.main-menu.links %}
    {% capture link_title %}{% comment %}ly_code_replace_for_[ link.title | strip_html ]_begin{% endcomment %}{% include 'ly-title' with link %}{{ ly_translation | strip_html }}{% comment %}ly_code_replace_end{% endcomment %}{% endcapture %}

    {% if link_title == settings.nav_uber_name %}

      {% for i in (1..4) %}
        {% capture uber_list_list_key %}nav_uber_list_{{i}}{% endcapture %}
        {% if settings[uber_list_list_key] != blank %}
          {% assign child_list_handle = settings[uber_list_list_key] %}
          {% include 'mobile-navigation-sublist' %}
  
  		  {% assign child_list_handle = settings[uber_list_list_key] %}
          {% for l in linklists[child_list_handle].links %}
            {% capture l_title %}{% comment %}ly_code_replace_for_[ l.title | strip_html ]_begin{% endcomment %}{% include 'ly-title' with l %}{{ ly_translation | strip_html }}{% comment %}ly_code_replace_end{% endcomment %}{% endcapture %}
            {% include 'has-drop-down' with l_title %}
            {% if has_drop_down %}{% include 'mobile-navigation-sublist' %}{% endif %}
          {% endfor %}
        {% endif %}
      {% endfor %}

    {% else %}
    {% include 'has-drop-down' with link_title %}
    {% if has_drop_down %}
      {% capture store_child_list_handle %}{{ child_list_handle }}{% endcapture %}
  	  {% include 'mobile-navigation-sublist' %}
  
      {% for l in linklists[store_child_list_handle].links %}
        {% capture l_title %}{% comment %}ly_code_replace_for_[ l.title | strip_html ]_begin{% endcomment %}{% include 'ly-title' with l %}{{ ly_translation | strip_html }}{% comment %}ly_code_replace_end{% endcomment %}{% endcapture %}
        {% include 'has-drop-down' with l_title %}
  		{% if has_drop_down %}{% include 'mobile-navigation-sublist' %}{% endif %}
  	  {% endfor %}
    {% endif %}
    {% endif %}
  {% endfor %}
  
  {{ uber_subnav }}
</nav>